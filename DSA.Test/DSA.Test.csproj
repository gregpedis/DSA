	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\DSA\DSA.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Using Include="Microsoft.VisualStudio.TestTools.UnitTesting" />
